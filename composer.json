{
  "name": "xmarcos/php-weasyprint",
  "description": "‚ö†Ô∏è Backport for PHP 5.6 (end of life üëª) ‚Äî PHP library allowing PDF generation or snapshot from an URL or an HTML page. Wrapper for Kozea/WeasyPrint.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "weasyprint",
    "pdf",
    "php",
    "php56"
  ],
  "authors": [
    {
      "name": "Pontedilana Dev Team",
      "homepage": "https://www.pontedilana.it"
    },
    {
      "name": "Marcos Sader",
      "homepage": "https://xmarcos.com"
    }
  ],
  "require": {
    "php": "^5.6",
    "psr/log": "^1.1",
    "symfony/process": "^3.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2",
    "phpunit/phpunit": "^5"
  },
  "autoload": {
    "psr-4": {
      "xmarcos\\PhpWeasyPrint\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Unit\\xmarcos\\PhpWeasyPrint\\": "tests/Unit/"
    }
  },
  "scripts": {
    "check-cs": "vendor/bin/php-cs-fixer fix --diff --dry-run --verbose",
    "fix-cs": "vendor/bin/php-cs-fixer fix --verbose",
    "unit-tests": "vendor/bin/phpunit"
  }
}
